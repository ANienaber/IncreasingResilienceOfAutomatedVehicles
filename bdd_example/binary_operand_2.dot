digraph{
    graph [margin=0];
    l1 [label = "0 and 0"]
    l2 [label = "0 and 1"]
    l3 [label = "1 and 0"]
    l4 [label = "1 and 1"]
    y1 [label = "x2"]
    y2 [label = "x2"]
    x [label = x]
    
    x -> y1[style = dashed]
    x -> y2 
    y1 -> l1[style = dashed]
    y1 -> l2  
    y2 -> l3[style = dashed]
    y2 -> l4 
}