digraph{
    graph [margin=0];
    z1 [label = "z",style=filled, fillcolor = "#dac4f7"]
    #z2 [label = "z"]
    #z3 [label = "z"]
    #z4 [label = "z"]
    y1 [label = "y"]
    y2 [label = "y"]
    x [label = x]
    l0 [label = "0"]
    l1 [label = "1"]


    x -> y2[style = dashed]
    x -> y1 
    y1 -> z1[style = dashed]
    y1 -> l1 
    y2 -> l0[style = dashed]
    y2 -> z1 
    z1 -> l0[style = dashed]
    z1 -> l1  
    
}